\n    }\n\n    private initCalendar() {\n        const initialDate = this.date ? this.date : undefined;\n        return bulmaCalendar.attach(this.createRef(), {\n            startDate: initialDate,\n            dateFormat: 'DD-MM-YYYY',\n            displayMode: 'dialog'\n        })[0];\n    }\n\n    private createRef() {\n        return this.$refs.date;\n    }\n\n}\n"],"sourceRoot":""}]}