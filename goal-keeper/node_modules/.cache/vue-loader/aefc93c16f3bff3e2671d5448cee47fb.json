script lang=\"ts\">\nimport {Component, Emit, Prop, Vue} from 'vue-property-decorator';\nimport PlayerDto from \"@/data/dto/PlayerDto\";\nimport ClubTeamDto from \"@/data/dto/ClubTeamDto\";\nimport {namespace} from \"vuex-class\";\nimport PlayerBackend from \"@/backend/PlayerBackend\";\n\nconst ClubTeamModule = namespace(\"ClubTeamModule\");\n\n@Component\nexport default class PlayerPopup extends Vue {\n\n    @Prop()\n    player: PlayerDto | undefined;\n\n    @ClubTeamModule.State('clubTeam')\n    private clubTeam!: ClubTeamDto;\n\n    private showPopup = true;\n\n    private removePlayer() {\n      if (this.player) {\n        PlayerBackend.deletePlayer(this.player)\n            .toPromise()\n            .finally(() => {\n              this.$router.go(0);\n            });\n      }\n    }\n\n    @Emit('close-modal')\n    private closeModal() {\n      return true;\n    }\n\n}\n</script>\n\n<style scoped lang=\"scss\">\n\n.notification.is-warning[data-v-8a418d30] {\n  background-color: darkred;\n  color: rgb(255 255 255 / 70%);\n}\n\n.modal-background {\n  background-color: rgba(0, 0, 0, 0.46);\n}\n\n.button.is-light {\n  margin: 5px;\n}\n.control {\n  margin-top: 20px;\n}\n.fas fa-trash-alt {\n  margin-left: 20px;\n}\n</style>\n"]}]}