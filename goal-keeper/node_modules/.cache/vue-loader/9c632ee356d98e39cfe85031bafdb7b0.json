dia\">\n                <div class=\"media-content\">\n                    <span class=\"nowrap\"><p class=\"has-text-weight-semibold\"> {{ fullName }}</p></span>\n                    <span v-if=\"this.player.position === 'DEFENDER'\" class=\"icon-text has-text-primary\">\n                        <span class=\"icon\"><i class=\"fas fa-shield-alt\"></i></span><span>Defender</span>\n                    </span>\n                    <span v-if=\"this.player.position === 'MIDFIELDER'\" class=\"icon-text has-text-info\">\n                        <span class=\"icon\"><i class=\"fas fa-running\"></i></span><span>Midfielder</span>\n                    </span>\n                    <span v-if=\"this.player.position === 'GOALKEEPER'\" class=\"icon-text has-text-danger\">\n                        <span class=\"icon\"><i class=\"fas fa-bullseye\"></i></span><span>GoalKeeper</span>\n                    </span>\n                    <span v-if=\"this.player.position === 'STRIKER'\" class=\"icon-text has-text-warning\">\n                        <span class=\"icon\"><i class=\"fas fa-bolt\"></i></span><span>Striker</span>\n                    </span>\n                </div>\n                <div class=\"media-right\">\n                  <ShirtIcon :shirt-number=\"this.player.shirtNumber\"></ShirtIcon>\n                </div>\n            </div>\n        </div>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport {Component, Emit, Prop, Vue} from 'vue-property-decorator';\nimport PlayerDto from '@/data/dto/PlayerDto';\nimport Routes from '@/router/Routes';\nimport ShirtIcon from '@/components/common//ShirtIcon.vue';\n\n@Component({\n    components: {\n        ShirtIcon\n    }\n})\nexport default class PlayerComponent extends Vue {\n    @Prop() private player! : PlayerDto;\n\n    @Emit('return-player')\n    public returnPlayer(): PlayerDto {\n        return this.player;\n    }\n\n    get fullName(): string {\n      return this.player.givenName + ' ' + this.player.surname\n    }\n\n    private editPlayer() {\n        if (this.player) {\n            this.$router.push({\n                name: Routes.Player,\n                params: {\n                playerId: this.player.id!\n            }});\n        }\n    }\n}\n\n</script>\n\n<style scoped lang=\"scss\">\n\n.card {\n  max-width: 500px;\n  margin: auto;\n  padding: 0.45rem;\n}\n\n@media (max-width: 769px) {\n  .card {\n    margin-right: 20px;\n    margin-left: 20px;\n  }\n}\n\n.card-content {\n    background-color: transparent;\n    padding: 0.4rem;\n    font-size: 0.9em;\n    height: 70px;\n\n    .media {\n        display: flex;\n        align-items: center;\n\n        .media-left > * {\n            margin: 5px;\n        }\n    }\n}\n\n.title {\n    font-size: 1.6rem;\n}\n\n.has-text-weight-semibold {\n    margin-left: 5px;\n    font-weight: 500 !important;\n}\n\n.has-text-link {\n    color: #33afa2 !important;\n}\n\n.columns.is-mobile > .column.is-full {\n  flex: none;\n  width: 100%;\n  margin-bottom: -11px;\n}\n\n.nowrap {\n  white-space: nowrap;\n}\n</style>\n"]}]}